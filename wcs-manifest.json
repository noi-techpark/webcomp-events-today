    "scripts": []
  },
  "configuration": {
    "tagName": "events-today",
    "options": [
      {
        "key": "event-location",
        "type": "text",
        "label": "Location of the events: NOI, EC",
        "required": true,
        "options": {
          "default": "NOI"
        }
      },
      {
        "key": "room",
        "type": "text",
        "label": "Room",
        "required": false,
        "options": {
          "default": ""
        }
      },
      {
        "key": "image-gallery-url",
        "type": "text",
        "label": "Gallery image s3 bucket URL",
        "required": false,
        "options": {
          "default": ""
        }
      },
      {
        "key": "image-gallery-interval",
        "type": "Number",
        "label": "Gallery interval in seconds",
        "required": false,
        "options": {
          "default": 60
        }
      }
    ]
  }
}
